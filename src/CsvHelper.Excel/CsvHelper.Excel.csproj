<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>netcoreapp3.1</TargetFramework>
		<Description>An implementation of IParser and ISerializer from CsvHelper that reads and writes using the EPPlus library.</Description>
		<AssemblyTitle>CsvHelper for Excel</AssemblyTitle>
		<VersionPrefix>2.1.0</VersionPrefix>
		<VersionSuffix>lethek</VersionSuffix>
		<Authors>Chris Rodgers</Authors>
		<DebugType>portable</DebugType>
		<AssemblyName>CsvHelper.Excel</AssemblyName>
		<OutputType>Library</OutputType>
		<PackageId>CsvHelper.Excel</PackageId>
		<PackageTags>CSV;CSVHelper;Excel;EPPlus</PackageTags>
		<PackageProjectUrl>https://github.com/lethek/CsvHelper.Excel</PackageProjectUrl>
		<PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
		<GeneratePackageOnBuild>false</GeneratePackageOnBuild>
		<LangVersion>latest</LangVersion>
		<AssemblyVersion>2020.10.22.1217</AssemblyVersion>
		<FileVersion>2020.10.22.1217</FileVersion>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="EPPlus" Version="5.4.0" />
	</ItemGroup>
	<ItemGroup>
		<ProjectReference Include="..\..\CsvHelper\src\CsvHelper\CsvHelper.csproj" />
	</ItemGroup>
</Project>